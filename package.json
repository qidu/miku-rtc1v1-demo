{
  "name": "pili-player-demo",
  "version": "0.0.1",
  "scripts": {
    "start": "fec-builder -p 9000",
    "build": "webpack --config webpack.config.js",
    "lint": "tsc --noEmit && eslint --ignore-path .gitignore --ext .ts,.tsx ./",
    "test": "jest --coverage",
    "test:dev": "jest --collectCoverage=false -i --watch",
    "clear": "jest --clearCache",
    "snapshot": "jest -u",
    "validate": "npm run lint && npm run test",
    "travis:before-script": "yarn install --network-timeout 1000000 --ignore-engines --network-concurrency 1",
    "travis:script": "npm run validate"
  },
  "dependencies": {
    "formstate-x": "^2.0.1",
    "mobx": "^6.3.3",
    "mobx-react": "^7.2.0",
    "qn-rtplayer-web": "~1.2.6",
    "query-string": "~7.1.1",
    "react": "~17.0.1",
    "react-dom": "~17.0.1",
    "react-icecream": "npm:react-icecream@^2.0.0",
    "xgplayer": "~3.0.10",
    "xgplayer-flv.js": "~3.0.10",
    "xgplayer-hls.js": "~3.0.10"
  },
  "devDependencies": {
    "@qiniu/eslint-config": "^0.0.6",
    "@qiniu/typed-less-modules": "^0.1.2",
    "@types/node": "^12.0.0",
    "@types/react": "~17.0.1",
    "@types/react-dom": "~17.0.1",
    "@types/react-test-renderer": "~16.9.3",
    "@typescript-eslint/eslint-plugin": "~4.10.0",
    "css-loader": "^7.1.2",
    "eslint": "~7.2.0",
    "eslint-import-resolver-typescript": "~2.3.0",
    "eslint-plugin-import": "~2.22.1",
    "eslint-plugin-jsx-a11y": "~6.3.0",
    "eslint-plugin-react": "~7.20.0",
    "eslint-plugin-react-hooks": "~4.2.0",
    "html-webpack-plugin": "^5.6.4",
    "jest": "~26.6.1",
    "less": "^4.4.2",
    "less-loader": "^12.3.0",
    "react-test-renderer": "~17.0.1",
    "style-loader": "^4.0.0",
    "ts-jest": "~26.4.2",
    "ts-loader": "^9.5.4",
    "typescript": "~4.1.5",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "whatwg-fetch": "~3.0.0"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "private": true
}
